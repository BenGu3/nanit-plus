{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build:css": "bunx tailwindcss -i ./src/index.css -o ./dist/index.css --minify",
    "build:js": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "build": "bun run build:css && bun run build:js",
    "start": "NODE_ENV=production bun src/index.ts",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.12",
    "bun-plugin-tailwind": "0.1.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lodash": "4.17.21",
    "lucide-react": "0.562.0",
    "luxon": "3.7.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "7.11.0",
    "recharts": "3.6.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/cli": "4.1.18",
    "@types/bun": "1.3.5",
    "@types/lodash": "4.17.21",
    "@types/luxon": "3.7.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vercel/node": "5.5.16",
    "autoprefixer": "10.4.23",
    "husky": "9.1.7",
    "prettier": "3.7.4",
    "tailwindcss": "4.1.18"
  }
}
